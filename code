CREATE SCHEMA `büchertausch_app`;

CREATE TABLE `ausleihdauer` (
  `DauerID` int NOT NULL,
  `DauerInTagen` int NOT NULL,
  PRIMARY KEY (`DauerID`),
  UNIQUE KEY `DauerInTagen_UNIQUE` (`DauerInTagen`)
);

CREATE TABLE `autor` (
  `AutorID` int NOT NULL,
  `Vorname` varchar(100) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  `Nachname` varchar(100) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`AutorID`)
);

CREATE TABLE `benutzende` (
  `BenutzerID` int NOT NULL,
  `Vorname` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  `Nachname` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  `Emailadresse` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  `Passwort` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  `Telefonnummer` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  PRIMARY KEY (`BenutzerID`),
  UNIQUE KEY `Emailadresse_UNIQUE` (`Emailadresse`),
  UNIQUE KEY `Passwort_UNIQUE` (`Passwort`)
);

CREATE TABLE `benutzerrolle` (
  `RolleID` int NOT NULL,
  `RollenName` varchar(100) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`RolleID`),
  UNIQUE KEY `RollenName_UNIQUE` (`RollenName`)
);

CREATE TABLE `genre` (
  `GenreID` int NOT NULL,
  `GenreName` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  PRIMARY KEY (`GenreID`),
  UNIQUE KEY `GenreName_UNIQUE` (`GenreName`)
);

CREATE TABLE `land` (
  `LandID` int NOT NULL,
  `LandName` varchar(100) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`LandID`),
  UNIQUE KEY `LandName_UNIQUE` (`LandName`)
);

CREATE TABLE `ort` (
  `OrtID` int NOT NULL,
  `PLZ` varchar(20) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  `Stadt` varchar(100) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`OrtID`),
  UNIQUE KEY `PLZ_UNIQUE` (`PLZ`)
);

CREATE TABLE `verlag` (
  `VerlagID` int NOT NULL,
  `VerlagName` varchar(100) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`VerlagID`),
  UNIQUE KEY `VerlagName_UNIQUE` (`VerlagName`)
);

CREATE TABLE `versandoptionen` (
  `VersandID` int NOT NULL,
  `Beschreibung` varchar(255) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`VersandID`),
  UNIQUE KEY `Beschreibung_UNIQUE` (`Beschreibung`)
);


CREATE TABLE `zeitslots` (
  `ZeitslotID` int NOT NULL,
  `Wochentag` text COLLATE utf8mb4_german2_ci,
  `Starzeit` time DEFAULT NULL,
  `Endzeit` time DEFAULT NULL,
  PRIMARY KEY (`ZeitslotID`)
);

CREATE TABLE `zustand` (
  `ZustandID` int NOT NULL,
  `ZustandName` varchar(100) COLLATE utf8mb4_german2_ci NOT NULL,
  `ZustandsBeschreibung` varchar(500) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`ZustandID`),
  UNIQUE KEY `ZustandsBeschreibung_UNIQUE` (`ZustandsBeschreibung`)
);

CREATE TABLE `adresse` (
  `AdresseID` int NOT NULL,
  `LandID` int NOT NULL,
  `OrtID` int DEFAULT NULL,
  `BenutzerID` int NOT NULL,
  `Strasse` varchar(45) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  PRIMARY KEY (`AdresseID`),
  KEY `LandID_idx` (`LandID`),
  KEY `OrtID_idx` (`OrtID`),
  KEY `BenutzerID_idx` (`BenutzerID`),
  CONSTRAINT `BenutzerID` FOREIGN KEY (`BenutzerID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `LandID` FOREIGN KEY (`LandID`) REFERENCES `land` (`LandID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `OrtID` FOREIGN KEY (`OrtID`) REFERENCES `ort` (`OrtID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `ausleihe` (
  `AusleiheID` int NOT NULL,
  `Ausleihdatum` date DEFAULT NULL,
  `Rückgabedatum` date DEFAULT NULL,
  `AusleiherID` int DEFAULT NULL,
  `ExemplarID` int DEFAULT NULL,
  PRIMARY KEY (`AusleiheID`),
  KEY `ExemplarID_idx` (`ExemplarID`),
  KEY `AusleiherID_idx` (`AusleiherID`),
  CONSTRAINT `ausleiherID` FOREIGN KEY (`AusleiherID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `ExemplarID` FOREIGN KEY (`ExemplarID`) REFERENCES `buchexemplar` (`ExemplarID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `autoren_schreiben_bücher` (
  `AutorenBuecherID` int NOT NULL,
  `AutorID` int NOT NULL,
  `BuchtitelID` int NOT NULL,
  PRIMARY KEY (`AutorenBuecherID`),
  KEY `BuchtitelID_idx` (`BuchtitelID`),
  KEY `AutorID_idx` (`AutorID`),
  CONSTRAINT `AutorID` FOREIGN KEY (`AutorID`) REFERENCES `autor` (`AutorID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `BuchtitelID` FOREIGN KEY (`BuchtitelID`) REFERENCES `buchtitel` (`BuchID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `benutzende_haben_benutzerrollen` (
  `idbenutzende_benutzerrollenID` int NOT NULL,
  `BenutzendeID` int NOT NULL,
  `RolleID` int NOT NULL,
  PRIMARY KEY (`idbenutzende_benutzerrollenID`),
  KEY `BenutzerID_idx` (`BenutzendeID`),
  KEY `RolleID_idx` (`RolleID`),
  CONSTRAINT `BenutzendeID` FOREIGN KEY (`BenutzendeID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `RolleID` FOREIGN KEY (`RolleID`) REFERENCES `benutzerrolle` (`RolleID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `benutzer_versandoptionen` (
  `benutzer_Versandoptionen` int NOT NULL,
  `UserID` int DEFAULT NULL,
  `VersandID` int DEFAULT NULL,
  PRIMARY KEY (`benutzer_Versandoptionen`),
  KEY `UserID_idx` (`UserID`),
  KEY `VersandID_idx` (`VersandID`),
  CONSTRAINT `UserID` FOREIGN KEY (`UserID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `VersandID` FOREIGN KEY (`VersandID`) REFERENCES `versandoptionen` (`VersandID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `benutzer_zeitslot` (
  `Benutzer_ZeitSlotID` int NOT NULL,
  `AnbieterID` int DEFAULT NULL,
  `ZeitslotID` int DEFAULT NULL,
  PRIMARY KEY (`Benutzer_ZeitSlotID`),
  KEY `BenutzerID_idx` (`AnbieterID`),
  KEY `Zeitslot_idx` (`ZeitslotID`),
  CONSTRAINT `AnbieterID` FOREIGN KEY (`AnbieterID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `Zeitslot` FOREIGN KEY (`ZeitslotID`) REFERENCES `zeitslots` (`ZeitslotID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `bewertung` (
  `BewertungsID` int NOT NULL,
  `Bewertungstext` text COLLATE utf8mb4_german2_ci,
  `Bewertungspunktzahl` int DEFAULT NULL,
  `Bewertungsdatum` date DEFAULT NULL,
  `LeserID` int DEFAULT NULL,
  `BuchNameID` int DEFAULT NULL,
  PRIMARY KEY (`BewertungsID`),
  KEY `BuchNameID_idx` (`BuchNameID`) /*!80000 INVISIBLE */,
  KEY `LeserID_idx` (`LeserID`),
  CONSTRAINT `BuchName` FOREIGN KEY (`BuchNameID`) REFERENCES `buchtitel` (`BuchID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `LeserID` FOREIGN KEY (`LeserID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `buchexemplar` (
  `ExemplarID` int NOT NULL,
  `BuchID` int DEFAULT NULL,
  `NutzerID` int DEFAULT NULL,
  `ZustandID` int DEFAULT NULL,
  PRIMARY KEY (`ExemplarID`),
  KEY `BuchlD_idx` (`BuchID`),
  KEY `NutzerID_idx` (`NutzerID`),
  KEY `ZustandID_idx` (`ZustandID`),
  CONSTRAINT `BuchID` FOREIGN KEY (`BuchID`) REFERENCES `buchtitel` (`BuchID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `NutzerID` FOREIGN KEY (`NutzerID`) REFERENCES `benutzende` (`BenutzerID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `ZustandID` FOREIGN KEY (`ZustandID`) REFERENCES `zustand` (`ZustandID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `buchtitel` (
  `BuchID` int NOT NULL,
  `TitelName` varchar(255) COLLATE utf8mb4_german2_ci NOT NULL,
  `ISBN` varchar(20) COLLATE utf8mb4_german2_ci DEFAULT NULL,
  `JahrDerVeroeffentlichung` year NOT NULL,
  `VerlagID` int DEFAULT NULL,
  PRIMARY KEY (`BuchID`),
  UNIQUE KEY `TitelName_UNIQUE` (`TitelName`),
  UNIQUE KEY `ISBN_UNIQUE` (`ISBN`),
  KEY `VerlagID_idx` (`VerlagID`),
  CONSTRAINT `VerlagID` FOREIGN KEY (`VerlagID`) REFERENCES `verlag` (`VerlagID`) ON DELETE CASCADE ON UPDATE CASCADE
);

CREATE TABLE `bücher_gehörenzu_genre` (
  `Buecher_Genre_ID` int NOT NULL,
  `BuchtitelID` int NOT NULL,
  `GenreID` int NOT NULL,
  PRIMARY KEY (`Buecher_Genre_ID`),
  KEY `BuchtitelD_idx` (`BuchtitelID`),
  KEY `GenreID_idx` (`GenreID`),
  CONSTRAINT `BuchtitelD` FOREIGN KEY (`BuchtitelID`) REFERENCES `buchtitel` (`BuchID`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `GenreID` FOREIGN KEY (`GenreID`) REFERENCES `genre` (`GenreID`) ON DELETE CASCADE ON UPDATE CASCADE
);


