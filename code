CREATE Database `büchertausch_app`;

CREATE TABLE `büchertausch_app`.`ausleihdauer` (
  `DauerID` INT NOT NULL,
  `DauerInTagen` INT NOT NULL,
  PRIMARY KEY (`DauerID`),
  UNIQUE INDEX `DauerInTagen_UNIQUE` (`DauerInTagen` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`autor` (
  `AutorID` INT NOT NULL,
  `Vorname` VARCHAR(100) NOT NULL,
  `Nachname` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`AutorID`));

CREATE TABLE `büchertausch_app`.`benutzende` (
  `BenutzerID` INT NOT NULL AUTO_INCREMENT,
  `Vorname` VARCHAR(100) NOT NULL,
  `Nachname` VARCHAR(100) NOT NULL,
  `Emailadresse` VARCHAR(100) NOT NULL,
  `Passwort` VARCHAR(100) NOT NULL,
  `Telefonnummer` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`BenutzerID`),
  UNIQUE INDEX `Emailadresse_UNIQUE` (`Emailadresse` ASC) VISIBLE,
  UNIQUE INDEX `Passwort_UNIQUE` (`Passwort` ASC) VISIBLE,
  UNIQUE INDEX `Telefonnummer_UNIQUE` (`Telefonnummer` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`benutzerrolle` (
  `RolleID` INT NOT NULL AUTO_INCREMENT,
  `RollenName` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`RolleID`),
  UNIQUE INDEX `RollenName_UNIQUE` (`RollenName` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`genre` (
  `GenreID` INT NOT NULL AUTO_INCREMENT,
  `GenreName` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`GenreID`),
  UNIQUE INDEX `GenreName_UNIQUE` (`GenreName` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`land` (
  `LandID` INT NOT NULL AUTO_INCREMENT,
  `LandName` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`LandID`),
  UNIQUE INDEX `LandName_UNIQUE` (`LandName` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`ort` (
  `OrtID` INT NOT NULL,
  `PLZ` VARCHAR(20) NOT NULL,
  `Stadt` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`OrtID`),
  UNIQUE INDEX `PLZ_UNIQUE` (`PLZ` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`verlag` (
  `VerlagID` INT NOT NULL,
  `VerlagName` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`VerlagID`),
  UNIQUE INDEX `VerlagName_UNIQUE` (`VerlagName` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`versandoptionen` (
  `VersandID` INT NOT NULL AUTO_INCREMENT,
  `Beschreibung` VARCHAR(255) NOT NULL,
  PRIMARY KEY (`VersandID`),
  UNIQUE INDEX `Beschreibung_UNIQUE` (`Beschreibung` ASC) VISIBLE);


CREATE TABLE `büchertausch_app`.`zeitslots` (
  `ZeitslotID` INT NOT NULL AUTO_INCREMENT,
  `Wochentag` TEXT NOT NULL,
  `Starzeit` TIME NOT NULL,
  `Endzeit` TIME NOT NULL,
  PRIMARY KEY (`ZeitslotID`));

CREATE TABLE `büchertausch_app`.`zustand` (
  `ZustandID` INT NOT NULL AUTO_INCREMENT,
  `ZustandName` VARCHAR(100) NOT NULL,
  `ZustandsBeschreibung` VARCHAR(500) NOT NULL,
  PRIMARY KEY (`ZustandID`),
  UNIQUE INDEX `ZustandsBeschreibung_UNIQUE` (`ZustandsBeschreibung` ASC) VISIBLE);

CREATE TABLE `büchertausch_app`.`adresse` (
  `AdresseID` INT NOT NULL AUTO_INCREMENT,
  `LandID` INT NOT NULL,
  `OrtID` INT NOT NULL,
  `BenutzerID` INT NOT NULL,
  `Strasse` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`AdresseID`),
  INDEX `BenutzerID_idx` (`BenutzerID` ASC) VISIBLE,
  INDEX `OrtID_idx` (`OrtID` ASC) VISIBLE,
  INDEX `LandID_idx` (`LandID` ASC) VISIBLE,
  CONSTRAINT `BenutzerID`
    FOREIGN KEY (`BenutzerID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `OrtID`
    FOREIGN KEY (`OrtID`)
    REFERENCES `büchertausch_app`.`ort` (`OrtID`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `LandID`
    FOREIGN KEY (`LandID`)
    REFERENCES `büchertausch_app`.`land` (`LandID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`buchtitel` (
  `BuchID` INT NOT NULL AUTO_INCREMENT,
  `TitelName` VARCHAR(255) NOT NULL,
  `ISBN` VARCHAR(20) NOT NULL,
  `JahrDerVeroeffentlichung` YEAR NOT NULL,
  `VerlagID` INT NOT NULL,
  PRIMARY KEY (`BuchID`),
  UNIQUE INDEX `TitelName_UNIQUE` (`TitelName` ASC) VISIBLE,
  UNIQUE INDEX `ISBN_UNIQUE` (`ISBN` ASC) VISIBLE,
  INDEX `VerlagID_idx` (`VerlagID` ASC) VISIBLE,
  CONSTRAINT `VerlagID`
    FOREIGN KEY (`VerlagID`)
    REFERENCES `büchertausch_app`.`verlag` (`VerlagID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`buchexemplar` (
  `ExemplarID` INT NOT NULL AUTO_INCREMENT,
  `BuchID` INT NOT NULL,
  `NutzerID` INT NOT NULL,
  `ZustandID` INT NOT NULL,
  PRIMARY KEY (`ExemplarID`),
  INDEX `BuchID_idx` (`BuchID` ASC) VISIBLE,
  INDEX `BenutzerID_idx` (`NutzerID` ASC) VISIBLE,
  INDEX `ZustandID_idx` (`ZustandID` ASC) VISIBLE,
  CONSTRAINT `BuchID`
    FOREIGN KEY (`BuchID`)
    REFERENCES `büchertausch_app`.`buchtitel` (`BuchID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `NutzerID`
    FOREIGN KEY (`NutzerID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `ZustandID`
    FOREIGN KEY (`ZustandID`)
    REFERENCES `büchertausch_app`.`zustand` (`ZustandID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`ausleihe` (
  `AusleiheID` INT NOT NULL AUTO_INCREMENT,
  `Ausleihdatum` DATE NOT NULL,
  `Rückgabedatum` DATE DEFAULT NULL,
  `AusleiherID` INT NOT NULL,
  `ExemplarID` INT NOT NULL,
  PRIMARY KEY (`AusleiheID`),
  INDEX `AusleiherID_idx` (`AusleiherID` ASC) VISIBLE,
  INDEX `ExemplarID_idx` (`ExemplarID` ASC) VISIBLE,
  CONSTRAINT `AusleiherID`
    FOREIGN KEY (`AusleiherID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `ExemplarID`
    FOREIGN KEY (`ExemplarID`)
    REFERENCES `büchertausch_app`.`buchexemplar` (`ExemplarID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`autoren_schreiben_bücher` (
  `AutorenBuecherID` INT NOT NULL AUTO_INCREMENT,
  `AutorID` INT NOT NULL,
  `BuchtitelID` INT NOT NULL,
  PRIMARY KEY (`AutorenBuecherID`),
  INDEX `AutorID_idx` (`AutorID` ASC) VISIBLE,
  INDEX `BuchtitelID_idx` (`BuchtitelID` ASC) VISIBLE,
  CONSTRAINT `AutorID`
    FOREIGN KEY (`AutorID`)
    REFERENCES `büchertausch_app`.`autor` (`AutorID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `BuchtitelID`
    FOREIGN KEY (`BuchtitelID`)
    REFERENCES `büchertausch_app`.`buchtitel` (`BuchID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`benutzende_haben_benutzerrollen` (
  `idbenutzende_benutzerrollenID` INT ZEROFILL NOT NULL,
  `BenutzendeID` INT NOT NULL,
  `RolleID` INT NOT NULL,
  PRIMARY KEY (`idbenutzende_benutzerrollenID`),
  INDEX `BenutzendeID_idx` (`BenutzendeID` ASC) VISIBLE,
  INDEX `RolleID_idx` (`RolleID` ASC) VISIBLE,
  CONSTRAINT `BenutzendeID`
    FOREIGN KEY (`BenutzendeID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `RolleID`
    FOREIGN KEY (`RolleID`)
    REFERENCES `büchertausch_app`.`benutzerrolle` (`RolleID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`benutzer_versandoptione` (
  `benutzer_VersandoptionenID` INT NOT NULL AUTO_INCREMENT,
  `UserID` INT NOT NULL,
  `VersandID` INT NOT NULL,
  PRIMARY KEY (`benutzer_VersandoptionenID`),
  INDEX `UserID_idx` (`UserID` ASC) VISIBLE,
  INDEX `VersandID_idx` (`VersandID` ASC) VISIBLE,
  CONSTRAINT `UserID`
    FOREIGN KEY (`UserID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `VersandID`
    FOREIGN KEY (`VersandID`)
    REFERENCES `büchertausch_app`.`versandoptionen` (`VersandID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

REATE TABLE `büchertausch_app`.`benutzer_zeitslot` (
  `Benutzer_ZeitSlotID` INT ZEROFILL NOT NULL,
  `AnbieterID` INT NOT NULL,
  `ZeitslotID` INT NOT NULL,
  PRIMARY KEY (`Benutzer_ZeitSlotID`),
  INDEX `AnbieterID_idx` (`AnbieterID` ASC) VISIBLE,
  INDEX `ZeitslotID_idx` (`ZeitslotID` ASC) VISIBLE,
  CONSTRAINT `AnbieterID`
    FOREIGN KEY (`AnbieterID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `ZeitslotID`
    FOREIGN KEY (`ZeitslotID`)
    REFERENCES `büchertausch_app`.`zeitslots` (`ZeitslotID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

CREATE TABLE `büchertausch_app`.`bewertung` (
  `BewertungsID` INT ZEROFILL NOT NULL,
  `Bewertungstext` TEXT NOT NULL,
  `Bewertungsdatum` DATE NOT NULL,
  `LeserID` INT NOT NULL,
  `BuchNameID` INT NOT NULL,
  PRIMARY KEY (`BewertungsID`),
  INDEX `BuchNameID_idx` (`BuchNameID` ASC) VISIBLE,
  INDEX `LeserID_idx` (`LeserID` ASC) VISIBLE,
  CONSTRAINT `BuchNameID`
    FOREIGN KEY (`BuchNameID`)
    REFERENCES `büchertausch_app`.`buchtitel` (`BuchID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `LeserID`
    FOREIGN KEY (`LeserID`)
    REFERENCES `büchertausch_app`.`benutzende` (`BenutzerID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);


CREATE TABLE `büchertausch_app`.`bücher_gehörenzu_genre` (
  `Buecher_Genre_ID` INT NOT NULL,
  `BuchtitelID` INT NOT NULL,
  `GenreID` INT NOT NULL,
  PRIMARY KEY (`Buecher_Genre_ID`),
  INDEX `BuchtitelD_idx` (`BuchtitelID` ASC) VISIBLE,
  INDEX `GenreID_idx` (`GenreID` ASC) VISIBLE,
  CONSTRAINT `BuchtitelD`
    FOREIGN KEY (`BuchtitelID`)
    REFERENCES `büchertausch_app`.`buchtitel` (`BuchID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `GenreID`
    FOREIGN KEY (`GenreID`)
    REFERENCES `büchertausch_app`.`genre` (`GenreID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);

